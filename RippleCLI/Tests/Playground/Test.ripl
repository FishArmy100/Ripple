
unsafe extern "C" func printf(char* msg) -> void;

func print(String& msg) -> void
{
    unsafe { printf(msg.c_str()); }
}

func println(String& msg) -> void
{
    unsafe 
    {
        print(msg);
        print("\n");
    }
}

